  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5a6a80",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["comment.block.documentation", "comment.block.javadoc"],
      "settings": {
        "foreground": "#6a7a90",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "keyword.control", "keyword.operator.new", "keyword.operator.expression", "keyword.operator.logical", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#5BBCD6"
      }
    },
    {
      "scope": ["keyword.operator", "keyword.operator.assignment", "keyword.operator.arithmetic", "keyword.operator.comparison"],
      "settings": {
        "foreground": "#a0a8a5"
      }
    },
    {
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#7BCCE6"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "entity.name.interface", "entity.name.enum", "entity.name.struct", "support.type", "support.class"],
      "settings": {
        "foreground": "#c09860"
      }
    },
    {
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#c09860"
      }
    },
    {
      "scope": ["variable", "variable.other", "variable.parameter", "variable.other.readwrite"],
      "settings": {
        "foreground": "#E8DCD0"
      }
    },
    {
      "scope": ["variable.language", "variable.other.constant"],
      "settings": {
        "foreground": "#e07080"
      }
    },
    {
      "scope": ["entity.name.tag", "meta.tag"],
      "settings": {
        "foreground": "#5BBCD6"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#E8DCD0"
      }
    },
    {
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#6ABA7A"
      }
    },
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#7ACA8A"
      }
    },
    {
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#8ADA9A"
      }
    },
    {
      "scope": ["constant.numeric", "constant.numeric.integer", "constant.numeric.float", "constant.numeric.hex"],
      "settings": {
        "foreground": "#e07080"
      }
    },
    {
      "scope": ["constant.language", "constant.language.boolean"],
      "settings": {
        "foreground": "#e07080"
      }
    },
    {
      "scope": ["constant.other"],
      "settings": {
        "foreground": "#e07080"
      }
    },
    {
      "scope": ["punctuation", "punctuation.definition", "punctuation.separator", "punctuation.terminator"],
      "settings": {
        "foreground": "#a0a8a5"
      }
    },
    {
      "scope": ["meta.brace", "punctuation.definition.block", "punctuation.definition.parameters"],
      "settings": {
        "foreground": "#a0a8a5"
      }
    },
    {
      "scope": ["support.constant", "support.variable"],
      "settings": {
        "foreground": "#e07080"
      }
    },
    {
      "scope": ["meta.decorator", "entity.name.function.decorator"],
      "settings": {
        "foreground": "#c09860"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#5BBCD6",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#7BCCE6"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#6ABA7A"
      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#6ABA7A"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#e07080"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#e0b050"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#e07080"
      }
    }
  ]
}
